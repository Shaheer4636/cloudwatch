# Use a valid name (no spaces)
export DASHBOARD_NAME="Strata-uat-eks-OwJIPjnj-cloudwatch"

# Create/update the dashboard with the JSON you already generated
aws cloudwatch put-dashboard \
  --region "$REGION" \
  --dashboard-name "$DASHBOARD_NAME" \
  --dashboard-body file://dashboard.json


aws cloudwatch list-dashboards --region "$REGION" \
  --query "DashboardEntries[?DashboardName=='$DASHBOARD_NAME']"
